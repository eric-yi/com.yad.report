<?xml version="1.0" encoding="UTF-8"?>
<!--
	.yadrptconfig
	报表全局配置文件
	包括：
		基础配置
		每个报表所在路径
-->
<yadrptconfig>
	<!-- 数据源xml定义 -->
	<yads>classpath:/com/yad/report/test/example.yads</yads>
	<reports>
		<load>init</load>
		<report>
			<path>classpath:/com/yad/report/test/example.yadrpt</path>
		</report>
	</reports>	

</yadrptconfig>